{
  "globalHeaders": {
    "content-security-policy": "default-src 'self';  script-src 'self' https://js.monitor.azure.com; upgrade-insecure-requests; frame-ancestors 'self'",
    "X-Frame-Options": "SAMEORIGIN",
    "X-Permitted-Cross-Domain-Policies": "none",
    "Referrer-Policy": "same-origin",
    "X-Content-Type-Options": "nosniff",
    "x-xss-protection": "1; mode=block",
    "Permissions-Policy": ""
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}